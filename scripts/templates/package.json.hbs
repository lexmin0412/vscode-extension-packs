{
  "name": "{{pack.name}}",
  "displayName": "{{titleCase pack.name}}",
  "description": "All extensions you need for {{packShortName}} development",
  "version": "{{pack.version}}",
  "engines": {
    "vscode": "^1.80.0"
  },
  "scripts": {
    "package": "vsce package"
  },
  "categories": [
    "Extension Packs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lexmin0412/vscode-extension-packs.git",
    "directory": "packages/{{pack.name}}"
  },
  "publisher": "lexmin0412",
  "extensionPack": [
    {{#each pack.extensions}}"{{this}}"{{#unless @last}},
    {{/unless}}{{/each}}
  ]
}
